{ #category : 'stepping' }
PannModel >> step [
	"one simulation day"
	
	<control>
	
	self updateDate.
	
	self @@ PannShellfishPopulation do: [:s | 
		s stepCohort.
		s recruit.
		 ].
	
	(self isFishingPeriod and: [ self isMbissa ]) ifTrue: [ 
		(self shuffled: (self @@ PannFisher))
			do: #goToFishingSpot;
			do: #fish.
		] ifFalse: [
		(self shuffled: (self @@ PannFisher))
			do: #goToVillage;
			do: [:fisher | fisher dailyEarnings: 0 ]. "there is no other activity"
		].
]

{ #category : 'time  ' }
PannModel >> updateDate [
    "Move forward one day and update the month accordingly"
    | daysInMonth |

	daysInMonth := #(31 28 31 30 31 30 31 31 30 31 30 31).  "Number of day per month"

   currentDayOfMonth := currentDayOfMonth + 1.
	dayCounter := dayCounter + 1.

   (currentDayOfMonth > (daysInMonth at: currentMonth)) ifTrue: [
		currentDayOfMonth := 1.
		currentMonth := (currentMonth \\ 12) + 1.
    ].
]
